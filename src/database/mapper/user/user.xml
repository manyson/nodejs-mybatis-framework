<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="USER">

    <select id="selectUser">
        SELECT *
        FROM TB_USER
        WHERE user_id = #{user_id}
    </select>

</mapper>
